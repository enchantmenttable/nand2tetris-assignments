/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=notsel);
    And(a=b, b=sel, out=andbsel);
    And(a=notsel, b=a, out=andasel);
    Or(a=andasel, b=andbsel, out=out);
}